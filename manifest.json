{
    "name": "Mobile.de NoSponsored",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Hides ads and sponsored listings",
    "icons": { "128": "icon.png" },
    "homepage_url": "https://github.com/aldiduzha",
    "content_scripts": [
        {
            "matches": ["http://*.mobile.de/*"],
            "css": ["mobile.css"]
        }
    ],
    "browser_action": {
    "default_icon": "icon.png"
  }
}
